      "required": false,
      "pluginOptions": {}
    },
    "language": {
      "type": "text"
    },
    "bookings": {
      "via": "helper_id",
      "collection": "bookings"
    },
    "date_time": {
      "type": "datetime"
    },
    "first_name": {
      "type": "string"
    },
    "last_name": {
      "type": "string"
    },
    "price": {
      "type": "integer"
    },
    "userID": {
      "type": "integer"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "helpers"
    }
  }
