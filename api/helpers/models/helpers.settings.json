{
  "kind": "collectionType",
  "collectionName": "helpers",
  "info": {
    "name": "Helpers",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "description": {
      "type": "text",
      "maxLength": 75
    },
    "profile_image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
